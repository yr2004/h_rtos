ROM_LOAD 0x80000000
{
;*******FLASH 0x8000 0000~~0x801F FFFF***********

    FLASH 0x80000000
    {
        Startup.o (Startup, +First)
        * (+RO)       
    }



;*******SDRAM 0x8100 0000~~0x8107 FFFF************

    HEAP 0x81000000  UNINIT
    {
        heap.o  (+ZI)
    }    
    STACK 0x81008000 UNINIT
    {
        stack.o (+ZI)
    }
    Sdram 0x81010000 0x8000 
    {
        * (+RW,+ZI)
    } 


;*******RAM 0x4000 0000~~0x4000 3FFF************

    VECTORS 0x40000000
    {
        vectors.o (Vect, +First)
    }
}

